# Configuración de la aplicación
NEXT_PUBLIC_API_URL=http://localhost:3001/api
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Configuración de Resend para envío de emails
# Obtén tu API key en: https://resend.com/api-keys
RESEND_API_KEY=tu_api_key_de_resend_aqui
RESEND_FROM_EMAIL=noreply@tudominio.com

# Configuración de OpenAI para extracción de PDF
# Obtén tu API key en: https://platform.openai.com/api-keys
OPENAI_API_KEY=tu_api_key_de_openai_aqui

# Configuración de n8n (opcional)
# URL del webhook de n8n para recibir datos procesados
N8N_WEBHOOK_URL=https://tu-instancia-n8n.com/webhook/tu-webhook-id
